<?xml version="1.0" encoding="utf-8"?>
<Ui xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.blizzard.com/wow/ui/" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\FrameXML\UI.xsd">
    <!-- Option -->
    <CheckButton name="hodoCheckboxTemplate" inherits="SettingsCheckboxControlTemplate" virtual="true"/>
    <Frame name="hodoDropdownTemplate" inherits="SettingsDropDownControlTemplate" virtual="true"/>
    <Slider name="hodoSliderTemplate" inherits="SettingsSliderControlTemplate" virtual="true"/>
    <Frame name="hodoCheckboxDropdownTemplate" inherits="SettingsCheckBoxDropDownControlTemplate" virtual="true"/>
    <Frame name="hodoCheckboxSliderTemplate" inherits="SettingsCheckBoxSliderControlTemplate" virtual="true"/>

    <!-- Frame -->
    <Frame name="hodoGameMenuFrame" inherits="MainMenuFrameTemplate" virtual="true"/> <!-- ESC 게임메뉴  -->
    <Frame name="hodoPopupTemplate" inherits="StaticPopupBaseTemplate" virtual="true"/> <!-- 아이템삭제  -->
    <Frame name="hodoDialogHeaderTemplate" inherits="DialogHeaderTemplate" virtual="true"/> <!-- 게임메뉴 헤더  -->

    <!-- Font  -->
    <Font name="hodoFontNormal" inherits="UserScaledFontGameNormal" virtual="true"/> <!-- 12, yellow  -->
    <Font name="hodoFontNormalL" inherits="UserScaledFontGameNormal" justifyH="LEFT" virtual="true"/>
    <Font name="hodoFontNormalR" inherits="UserScaledFontGameNormal" justifyH="RIGHT" virtual="true"/>
    <Font name="hodoFontNormalSmall" inherits="UserScaledFontGameNormalSmall" virtual="true"/> <!-- 10  -->
    <Font name="hodoFontNormalSmallL" inherits="UserScaledFontGameNormalSmall" justifyH="LEFT" virtual="true"/>
    <Font name="hodoFontNormalSmallR" inherits="UserScaledFontGameNormalSmall" justifyH="RIGHT" virtual="true"/>
    <Font name="hodoFontNormalLarge" inherits="UserScaledFontGameNormalLarge" virtual="true"/> <!-- 16  -->

    <Font name="hodoFontHighlight" inherits="UserScaledFontGameHighlight" virtual="true"/>  <!-- 12, white  -->
    <Font name="hodoFontHighlightL" inherits="UserScaledFontGameHighlight" justifyH="LEFT" virtual="true"/> 
    <Font name="hodoFontHighlightR" inherits="UserScaledFontGameHighlight" justifyH="RIGHT" virtual="true"/> 
    <Font name="hodoFontHighlightSmall" inherits="UserScaledFontGameHighlightSmall" virtual="true"/>  <!-- 10  -->
    <Font name="hodoFontHighlightSmallL" inherits="UserScaledFontGameHighlightSmall" justifyH="LEFT" virtual="true"/> 
    <Font name="hodoFontHighlightSmallR" inherits="UserScaledFontGameHighlightSmall" justifyH="RIGHT" virtual="true"/> 
    
    <Font name="hodoFontDisable" inherits="UserScaledFontGameDisable" virtual="true"/>  <!-- 12, gray  -->
    <Font name="hodoFontDisableL" inherits="UserScaledFontGameDisable" justifyH="LEFT" virtual="true"/> 
    <Font name="hodoFontDisableR" inherits="UserScaledFontGameDisable" justifyH="RIGHT" virtual="true"/> 
    <Font name="hodoFontDisableSmall" inherits="UserScaledFontGameDisableSmall" virtual="true"/>  <!-- 10  -->
    <Font name="hodoFontDisableSmallL" inherits="UserScaledFontGameDisableSmall" justifyH="LEFT" virtual="true"/> 
    <Font name="hodoFontDisableSmallR" inherits="UserScaledFontGameDisableSmall" justifyH="RIGHT" virtual="true"/> 
</Ui>   